<?xml version="1.0"?>
<extension key="ca.sdda.trialadmin" type="module">
  <file>trialadmin</file>
  <name>SDDA Trial Administration Module</name>
  <description>This module adds the functions needed to manage SDDA trial events</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Norm Dressler</author>
    <email>norm@sportingdetectiondogs.ca</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/cub69/ca.sdda.trialadmin/tree/master</url>
    <url desc="Documentation">https://github.com/cub69/ca.sdda.trialadmin/tree/master</url>
    <url desc="Support">https://github.com/cub69/ca.sdda.trialadmin/tree/master</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2021-06-11</releaseDate>
  <version>1.7</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.46.1</ver>
  </compatibility>
  <comments>In use for production</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Trialadmin</namespace>
    <format>22.05.2</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>ang-php@1.0.0</mixin>
  </mixins>
</extension>
